<%- include('./partials/header') -%>
	<body>
		<div  class="intro container-fluid">
			<div class="intro-div row-fluid">
				<div class="text-center">
					<h2><b>Dashboard</b></h2>
					<p class="h5">What would like to do today?</p>
					<a href=""><button class="btn btn-default">New Poll</button></a>
					<a href=""><button class="btn btn-default">My Polls</button></a>
				</div>
			</div>
		</div>

		<div class="container vote-container">
			<div class="row">
				<div class="col-md-6">
					<p class="h3">What is the name of your pet?<span class="h6"><br>by <span>Emmanuel</span><span></p>
				</div>
				<div class="col-md-6">
					<div id="canvas-holder">
						<canvas id="chart-area" width="500" height="500"/>
					</div>
				</div>
				
				
			</div>

		</div>
		<script>

		var doughnutData = [
				{
					value: 1,
					label: "One"
				},
				{
					value: 2,
					label: "Two"
				},
				{
					value: 3,
					label: "Three"
				},
				{
					value: 4,
					label: "Four"
				},
				{
					value: 5,
					label: "Five"
				}

			];

			window.onload = function(){
				var ctx = document.getElementById("chart-area").getContext("2d");
				window.myDoughnut = new Chart(ctx).Doughnut(doughnutData, {responsive : true});
			};



	</script>
	

	<%- include('./partials/footer') -%>
	</body>

	

</html>
